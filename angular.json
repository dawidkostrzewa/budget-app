{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "budget-ui": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "apps/budget-ui",
      "sourceRoot": "apps/budget-ui/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/budget-ui",
            "index": "apps/budget-ui/src/index.html",
            "main": "apps/budget-ui/src/main.ts",
            "polyfills": "apps/budget-ui/src/polyfills.ts",
            "tsConfig": "apps/budget-ui/tsconfig.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/budget-ui/src/favicon.ico",
              "apps/budget-ui/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/budget-ui/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/budget-ui/src/environments/environment.ts",
                  "with": "apps/budget-ui/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "UI:build:production"
            },
            "development": {
              "browserTarget": "UI:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "UI:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/budget-ui/src/test.ts",
            "polyfills": "apps/budget-ui/src/polyfills.ts",
            "tsConfig": "apps/budget-ui/tsconfig.spec.json",
            "karmaConfig": "apps/budget-ui/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/budget-ui/src/favicon.ico",
              "apps/budget-ui/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/budget-ui/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"],
            "tsConfig": [
              "apps/budget-ui/tsconfig.json",
              "apps/budget-ui/tsconfig.spec.json"
            ]
          }
        }
      }
    },
    "budget-backend": {
      "root": "apps/budget-backend",
      "sourceRoot": "apps/budget-backend/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/budget-backend",
            "main": "apps/budget-backend/src/main.ts",
            "tsConfig": "apps/budget-backend/tsconfig.app.json",
            "assets": ["apps/budget-backend/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/budget-backend/src/environments/environment.ts",
                  "with": "apps/budget-backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "backend:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/budget-backend/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/budget-backend"],
          "options": {
            "jestConfig": "apps/budget-backend/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "budget-models": {
      "root": "libs/shared/budget-models",
      "sourceRoot": "libs/shared/budget-models/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/shared/budget-models/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/budget-models"],
          "options": {
            "jestConfig": "libs/shared/budget-models/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  }
}
